{
  "name": "tab-butler",
  "version": "1.0.0-beta.1",
  "description": "A tab manager at your service",
  "main": "index.js",
  "scripts": {
    "start:chrome": "webpack --config webpack.config.js --watch",
    "build:chrome": "webpack --config webpack.prod.js",
    "lint": "eslint src",
    "format": "prettier --write .",
    "start:firefox": "cross-env BROWSER='firefox' webpack --config webpack.config.js --watch",
    "build:firefox": "cross-env BROWSER='firefox' webpack --config webpack.prod.js",
    "start:all": "concurrently --kill-others \"npm run start:chrome\" \"npm run start:firefox\"",
    "build:all": "concurrently --kill-others \"npm run build:chrome\" \"npm run build:firefox\""
  },
  "devDependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@react-icons/all-files": "^4.1.0",
    "focus-trap-react": "^9.0.2",
    "preact": "^10.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^2.16.4",
    "webextension-polyfill": "^0.9.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
