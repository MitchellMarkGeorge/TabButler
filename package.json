{
  "name": "tab-butler",
  "version": "1.0.0-beta.2",
  "description": "A tab manager at your service",
  "main": "index.js",
  "scripts": {
    "start:chrome": "webpack --config webpack.config.js --watch",
    "build:chrome": "webpack --config webpack.prod.js",
    "lint": "eslint src",
    "format": "prettier --write .",
    "start:firefox": "cross-env BROWSER='firefox' webpack --config webpack.config.js --watch",
    "build:firefox": "cross-env BROWSER='firefox' webpack --config webpack.prod.js",
    "start:all": "concurrently --kill-others \"npm run start:chrome\" \"npm run start:firefox\"",
    "build:all": "concurrently --kill-others \"npm run build:chrome\" \"npm run build:firefox\""
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/webextension-polyfill": "^0.9.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.3.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "sass-loader": "^13.0.2",
    "terser-webpack-plugin": "^5.3.5",
    "to-string-loader": "^1.2.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0",
    "zip-webpack-plugin": "^4.0.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@react-icons/all-files": "^4.1.0",
    "csstype": "^3.1.0",
    "focus-trap-react": "^9.0.2",
    "preact": "^10.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtuoso": "^2.17.0",
    "webextension-polyfill": "^0.10.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}
